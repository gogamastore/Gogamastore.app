{
  "expo": {
    "name": "Gogama Store",
    "slug": "gogama-store",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "gogama-store",
    "userInterfaceStyle": "automatic",
    "description": "Gogama Store - Aplikasi e-commerce untuk reseller",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/static-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF",
      "hideExpoSplash": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "store.gogama.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "icon": "./assets/images/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "store.gogama.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 4,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ],
      "enableProguardInReleaseBuilds": true,
      "enableHermes": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1d52d2a-940c-428d-8a2a-bafe6146b969"
      }
    }
  }
}
